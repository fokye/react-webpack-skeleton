    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.1",
    "chai-enzyme": "0.8.0",
    "css-loader": "0.28.5",
    "css-modules-require-hook": "4.0.6",
    "enzyme": "2.9.1",
    "eslint": "4.5.0",
    "eslint-loader": "1.9.0",
